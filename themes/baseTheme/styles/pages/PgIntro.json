    "&-flIconButton": {
      "flexProps": {
        "positionType": "RELATIVE"
      },
      "flex": {
        "positionType": 0
      }
    },
    "&-main": {
      "flexProps": {
        "justifyContent": "FLEX_END"
      }
    },
    "&-dotIndicator": {
      "flexProps": {
        "positionType": "RELATIVE"
      },
      "flex": {
        "positionType": 0
      },
      "height": 10,
      "width": 42,
      "marginTop": 20
    },
    "&-tvTitle": {
      "height": 96,
      "width": 315,
      "flexProps": {
        "alignSelf": "CENTER"
      },
      "marginBottom": 8,
      "lineSpacing": 10,
      "android": {
        "zIndex": 12
      }
    },
    "&-tvTitleDetail": {
      "height": 36,
