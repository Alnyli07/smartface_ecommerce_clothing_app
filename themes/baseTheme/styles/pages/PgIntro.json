{
  "#pgIntro": {
    "&_back": {
      "flexProps": {
        "positionType": "ABSOLUTE"
      },
      "top": 0,
      "height": null,
      "width": 430,
      "imageFillType": "STRETCH",
      "image": "intro_black_suit.png",
      "left": 0,
      "android": {
        "zIndex": 9
      },
      "bottom": 82
    },
    "&-headerBar": {
      "visible": false
    },
    "&-statusBar": {
      "visible": false,
      "android": {
        "color": "rgba( 6, 16, 35, 1 )"
      }
    },
    "&-backView": {
      "flexProps": {
        "positionType": "ABSOLUTE"
      },
      "left": 0,
      "top": 0,
      "right": 0,
      "height": null,
      "backgroundColor": {
        "startColor": "rgba(6, 16, 35, 0)",
        "endColor": "rgba( 6, 16, 35, 1 )",
        "direction": "VERTICAL"
      },
      "android": {
        "zIndex": 11
      },
      "bottom": 82
    },
    "&-flBottom": {
      "flexProps": {
        "positionType": "RELATIVE",
        "flexDirection": "ROW",
        "justifyContent": "SPACE_BETWEEN"
      },
      "height": 115,
      "backgroundColor": "rgba( 32, 41, 58, 1 )",
      "ios": {
        "shadowRadius": 20,
        "shadowOpacity": 0.67
      },
      "android": {
        "elevation": 20,
        "zIndex": 12
